{
  "version": 2,
  "name": "visor-markdown-vitaminado",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_OPENROUTER_API_KEY": "@openrouter_api_key"
  },
  "build": {
    "env": {
      "REACT_APP_OPENROUTER_API_KEY": "@openrouter_api_key"
    }
  },
  "functions": {
    "app/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
} 